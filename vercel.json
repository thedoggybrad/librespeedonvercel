{
  "functions": {
    "api/backend/*.php": {
      "runtime": "vercel-php@0.7.3",
      "maxDuration": 60
    },
    "api/docker/*.php": {
      "runtime": "vercel-php@0.7.3",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/(.*)", 
      "dest": "/api/$1"
    },
    {
      "src": "/backend/(.*)",
      "dest": "/api/backend/$1"
    },
    {
      "src": "/docker/(.*)",
      "dest": "/api/docker/$1"
    },
    {
      "src": "/(.*)", 
      "dest": "/index.html"
    }
  ]
}
